{
  "name": "gather-play",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gather-play.com",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "firebase": "firebase serve"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "axios": "^0.18.0",
    "canvas-text-wrapper": "^0.10.2",
    "firebase": "^5.5.3",
    "firebaseui": "^3.4.1",
    "howler": "^2.0.15",
    "leaflet": "^1.3.4",
    "lyrics.js": "^0.4.1",
    "register-service-worker": "^1.0.0",
    "three": "^0.97.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue2-leaflet": "^1.2.3",
    "vuetify": "^1.2.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "babel-plugin-transform-imports": "^1.5.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.2.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-cdn-plugin": "Neo-Zhixing/webpack-cdn-plugin"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "comma-dangle": [
        "warn",
        "only-multiline"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "plugins": [
      [
        "transform-imports",
        {
          "vuetify": {
            "transform": "vuetify/es5/components/${member}",
            "preventFullImport": true
          }
        }
      ]
    ]
  }
}
